<?xml version="1.0" encoding="utf-8"?>
<EasyUiXml>
    <name>巡查信息</name>
    <alias>巡查信息</alias>
    <uiXml>
        <UiXml>
            <alias>日志Id</alias>
            <code>Id</code>
            <dataType class="java-class">java.lang.String</dataType>
            <readonly>true</readonly>
            <VerifyXml>
                <canNull>false</canNull>
                <regex>^\d{32}$</regex>
                <msg>巡查日志主键必须32位字符串!</msg>
            </VerifyXml>
        </UiXml>
        <UiXml>
            <alias>巡查任务ID</alias>
            <code>XCRWID</code>
            <dataType class="java-class">java.lang.String</dataType>
            <readonly>true</readonly>
            <VerifyXml>
                <canNull>false</canNull>
                <regex>^\d{32}$</regex>
                <msg>巡查任务ID必须32位字符串!</msg>
            </VerifyXml>
        </UiXml>
        <UiXml>
            <alias>古树电子编号</alias>
            <code>DZBQH</code>
            <dataType class="java-class">java.lang.String</dataType>
            <readonly>true</readonly>
        </UiXml>
        <UiXml>
            <alias>树种</alias>
            <code>SZK</code>
            <dataType class="java-class">java.lang.String</dataType>
            <readonly>true</readonly>
        </UiXml>
        <UiXml>
            <alias>古树编号</alias>
            <code>GSBH</code>
            <dataType class="java-class">java.lang.String</dataType>
            <readonly>true</readonly>
        </UiXml>
        <UiXml>
            <alias>任务号</alias>
            <code>RWH</code>
            <dataType class="java-class">java.lang.String</dataType>
            <readonly>true</readonly>
        </UiXml>
        <UiXml>
            <alias>县（市、区）</alias>
            <code>XIAN</code>
            <dataType class="java-class">java.lang.String</dataType>
            <readonly>true</readonly>
        </UiXml>
        <UiXml>
            <alias>巡查问题(限500字)</alias>
            <code>PatrolContent</code>
            <dataType class="java-class">java.lang.String</dataType>
            <VerifyXml>
                <canNull>false</canNull>
                <regex>^(?=.*\S).+$</regex>
                <msg>不能为空!</msg>
            </VerifyXml>
        </UiXml>
        <!--巡查信息级别-->
        <UiXml>
            <alias>巡查信息级别</alias>
            <code>PatrolLevel</code>
            <dataType class="java-class">java.lang.String</dataType>
            <itemXml>
                <ItemXml>
                    <code class="string">1</code>
                    <value>正常</value>
                </ItemXml>
                <ItemXml>
                    <code class="string">2</code>
                    <value>较重</value>
                </ItemXml>
                <ItemXml>
                    <code class="string">3</code>
                    <value>严重</value>
                </ItemXml>
                <ItemXml>
                    <code class="string">4</code>
                    <value>紧急</value>
                </ItemXml>
            </itemXml>
            <VerifyXml>
                <canNull>false</canNull>
                <regex>^(?=.*\S).+$</regex>
                <msg>不能为空!</msg>
            </VerifyXml>
        </UiXml>
        <UiXml>
            <alias>填写时间</alias>
            <code>PatrolDate</code>
            <dataType class="java-class">java.lang.String</dataType>
        </UiXml>
        <UiXml>
            <alias>数据状态</alias>
            <code>VALID</code>
            <dataType class="java-class">java.lang.Integer</dataType>
        </UiXml>
        <UiXml>
            <alias>现场照片</alias>
            <code>GSZP</code>
            <dataType class="java-class">java.lang.String</dataType>
            <uiType>img</uiType>
        </UiXml>
    </uiXml>
</EasyUiXml>