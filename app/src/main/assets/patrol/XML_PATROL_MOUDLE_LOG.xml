                <canNull>false</canNull>
                <regex>
                    ^[EW]?((\d|[1-9]\d|1[0-7]\d)[s\°](\d|[0-5]\d)[s\′](\d|[0-5]\d)(\.\d{2,2})[s\″]$)
                </regex>
                <msg>请输入正确的经度!</msg>
            </VerifyXml>
